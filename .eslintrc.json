{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "plugin:vue/recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/max-attributes-per-line": 1
    }
}